class Solution {
public:
    void sortColors(vector<int>& nums) {int a=nums.size();
      for(int step=0; step<a;++step)
      {
          for(int i=0;i<a-step-1;++i)
          {
              
              if(nums[i]>nums[i+1])
              {
                  swap(nums[i],nums[i+1]);
              }
          }
      }
        
    }
};
