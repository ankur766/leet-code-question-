class Solution {
public:
    int findDuplicate(vector<int>& nums) {
  
       sort(nums.begin(),nums.end());
        int i=0;
       while(i<nums.size())
       {int j=i+1;
        if(nums[i]==nums[j])
        {
            return nums[i];
        }
        i++;
           
           
           
           
       }
        
  return NULL;        
    }      
        
    
};
